  {
    "type": "event",
    "name": "RaffleEnter",
    "anonymous": false,
    "inputs": [
      { "indexed": true,  "name": "_player",        "type": "address" },
      { "indexed": false, "name": "_ipHash",        "type": "bytes32" },
      { "indexed": false, "name": "_country3",      "type": "bytes3" },
      { "indexed": false, "name": "_lastTimestamp", "type": "uint256" },
      { "indexed": false, "name": "cycle",          "type": "uint256" }
    ]
  },
  {
    "type": "event",
    "name": "WinnerPicked",
    "anonymous": false,
    "inputs": [
      { "indexed": false, "name": "cycle",             "type": "uint256" },
      { "indexed": false, "name": "playersBeforePick", "type": "uint256" },
      { "indexed": true,  "name": "winner",            "type": "address" }
    ]
  },
  {
    "type": "event",
    "name": "RaffleFundsReceived",
    "anonymous": false,
    "inputs": [
      { "indexed": true,  "name": "from",   "type": "address" },
      { "indexed": false, "name": "amount", "type": "uint256" }
    ]
  },
  {
    "type": "event",
    "name": "RafflePrizePaid",
    "anonymous": false,
    "inputs": [
      { "indexed": true,  "name": "winner", "type": "address" },
      { "indexed": false, "name": "amount", "type": "uint256" }
    ]
  },

